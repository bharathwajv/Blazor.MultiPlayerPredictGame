@inject IJSRuntime jsRuntime
@using System.Collections.Generic
<h3>GraphChart</h3>
<div id="chart"></div>

@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            List<double> Mult = new List<double> { 1.1, 2.1, 3.1 };
            List<string> Seconds = new List<string> { "1s", "2s", "3.s" };
            await jsRuntime.InvokeVoidAsync("generateLineChart", Mult, Seconds);
            this.StateHasChanged();
        }
    }
}
